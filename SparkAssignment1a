      "cell_type": "code",
      "source": [
        "def extract_entities(sent):\n",
        "  tokens = nltk.word_tokenize(sent)\n",
        "  pos = nltk.pos_tag(tokens)\n",
        "  tree = nltk.ne_chunk(pos, binary=False)\n",
        "  ents = []\n",
        "  for subtree in tree:\n",
        "        if isinstance(subtree, Tree):\n",
        "            label = subtree.label()\n",
        "            name = \" \".join(tok for tok, _ in subtree.leaves())\n",
        "            ents.append((label, name))\n",
        "  return ents"
      ],
      "metadata": {
        "id": "xiRy3hYsIBsj"
      },
      "execution_count": 22,
      "outputs": []
    },
